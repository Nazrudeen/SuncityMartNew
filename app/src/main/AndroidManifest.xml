<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suncitytechno.suncitymartnew">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.suncitytechnoe.suncitymartnew.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.suncitytechnoe.suncitymartnew.activity.WelcomeActivity" />
        <activity android:name="com.suncitytechnoe.suncitymartnew.activity.SigninActivity" />
        <activity android:name="com.suncitytechnoe.suncitymartnew.activity.SignupActivity" />
        <activity
            android:name="com.suncitytechnoe.suncitymartnew.activity.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>